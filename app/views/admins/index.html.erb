<div>
<%= render 'partials/wedding_nav', {wedding: @wedding} %>
</div><br>
<table class="table">
  <tr>
    <td>Wedding Party</td>
    <td>Manage Wedding Party</td>
  </tr>
  <% @admins.each do |a| %>
    <%if a.user != current_user %>
      <tr>
        <td><%= a.email %></td>
        <td>
          <a href="#"><span class="glyphicon glyphicon-pencil"></span></a>
          <%= link_to admin_path(a), method: :delete do %>
            <span class="glyphicon glyphicon-trash"></span>
          <%end%>
        </td>
      </tr>
    <% end %>
  <% end %>
</table>
