<%= form_for wedding, url: wedding_path(wedding), method: 'patch' do |f|%>
  <%= f.label 'Tell us your love story'%>
  <%= f.text_area :story, cols: "100", rows: "30", class: 'form-group' %>
  <%= f.file_field :images, multiple: :true %>
  <%= f.hidden_field :images_cache %><br>
  <%= f.submit "Update", class: 'update-btn btn btn-default'%>
<%end%>
